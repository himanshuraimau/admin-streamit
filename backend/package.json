{
  "name": "streamit-admin-backend",
  "version": "1.0.0",
  "description": "streamit Admin Backend API",
  "main": "src/index.ts",
  "scripts": {
    "dev": "nodemon --watch src --ext ts,json --exec \"tsc && node dist/index.js\"",
    "build": "tsc",
    "start": "node dist/index.js",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:migrate": "prisma migrate dev",
    "db:studio": "prisma studio",
    "db:seed": "tsc && node --env-file=.env dist/seed.js",
    "db:reset": "prisma migrate reset --force && npm run db:seed"
  },
  "dependencies": {
    "@prisma/client": "^6.19.0",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/ms": "^2.1.0",
    "bcrypt": "^6.0.0",
    "bcryptjs": "^3.0.3",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "encodeurl": "^2.0.0",
    "escape-html": "^1.0.3",
    "express": "^5.1.0",
    "express-rate-limit": "^7.5.1",
    "jsonwebtoken": "^9.0.2",
    "ms": "^2.1.3",
    "on-finished": "^2.4.1",
    "parseurl": "^1.3.3",
    "proxy-addr": "^2.0.7",
    "range-parser": "^1.2.1",
    "vary": "^1.1.2",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/cors": "^2.8.17",
    "@types/express": "^5.0.0",
    "@types/node": "^20.17.6",
    "nodemon": "^3.1.0",
    "prisma": "^6.19.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.6.3"
  },
  "type": "module",
  "private": true
}